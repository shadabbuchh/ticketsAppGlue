{
  "name": "@app/openapi",
  "version": "0.0.1",
  "description": "OpenAPI specifications and generated types for app template",
  "type": "module",
  "packageManager": "pnpm@10.14.0",
  "engines": {
    "node": ">=22.17.1",
    "pnpm": "~10.14.0"
  },
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "generate-types": "node --import tsx scripts/generate-api-types.ts",
    "typecheck": "tsc --noEmit -p tsconfig.json",
    "prettier": "prettier --check . --cache",
    "prettier:fix": "prettier --write . --cache",
    "lint": "eslint . --cache",
    "lint:fix": "eslint . --fix"
  },
  "devDependencies": {
    "@eslint/js": "^9.0.0",
    "@stoplight/spectral-cli": "6.15.0",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^22.17.1",
    "eslint": "^9.0.0",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.29.1",
    "globals": "^15.0.0",
    "js-yaml": "^4.1.0",
    "openapi-typescript": "^7.9.1",
    "orval": "^7.11.2",
    "tsx": "^4.20.3",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.0.0"
  }
}
